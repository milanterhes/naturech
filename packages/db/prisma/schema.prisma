generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model User {
  id               String   @id @default(auto()) @map("_id") @db.ObjectId
  email            String
  registered       DateTime @default(now())
  lastLoginRequest DateTime @default(now())
}
